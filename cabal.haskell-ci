branches: master
doctest: <9
docspec: >=7.6
benchmarks: >=7.4 && <9
ghcjs-tests: True
apt: hugs libhugs-time-bundled libtestu01-0-dev

-- 9.2 testing
ghcup-jobs: >=7.10

constraint-set time-1.10
  ghc: >=8.0
  constraints: time ^>=1.10

raw-travis:
  export CABAL
  export HC
  if [ ! $GHCJS ]; then (cd ${PKGDIR_splitmix} && sh test-hugs.sh); fi
